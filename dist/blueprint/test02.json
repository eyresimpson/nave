{
  "label": "运行测试2",
  "version": "1.0",
  "request": {
    "engine": "1.0",
    "os": "any"
  },
  "flowType": "service",
  "port": "8099",
  "path": "demo",
  "vars": [{
    "name": "someone",
    "type": "String",
    "default": "Hello"
  }],
  "route": [
    {
      "name": "test",
      "path": "test1",
      "type": "Get",
      "getParams": [
        "user"
      ]
    },
    {
      "name": "test2",
      "path": "test2",
      "type": "Get",
      "getParams": [
        "user"
      ]
    }
  ],
  "mods": [
    "noah"
  ],
  "steps": [
    {
      "sid": "/demo/test1",
      "type": 0,
      "path": "common.print",
      "name": "Print",
      "optParams": [
        "[0] - hello world"
      ],
      "optReturn": [],
      "next": "/demo/test2"
    },
    {
      "sid": "/demo/test2",
      "type": 0,
      "path": "common.ret",
      "name": "Print",
      "optParams": [
        "[1] - #{name} 你好好 #{user}"
      ],
      "next": "2"
    },
    {
      "sid": "2",
      "next": "-1",
      "type": 1,
      "name": "IF",
      "condition": [
        "${1 == 1}?3,4:4",
        "${2 == 1}?4:3"
      ],
      "children": [
        {
          "sid": "3",
          "type": 0,
          "path": "common.print",
          "name": "Print",
          "optParams": [
            "[3] - hello world"
          ],
          "optReturn": []
        },
        {
          "sid": "4",
          "type": 1,
          "name": "IF",
          "condition": [
            "${1 == 1}?6,5:5",
            "${2 == 1}?5:6"
          ],
          "children": [
            {
              "sid": "6",
              "type": 0,
              "path": "common.print",
              "name": "Print",
              "optParams": [
                "[6] - hello world"
              ],
              "optReturn": []
            },
            {
              "sid": "5",
              "type": 0,
              "path": "common.print",
              "name": "Print",
              "optParams": [
                "[5] - hello world"
              ],
              "optReturn": []
            }
          ]
        }
      ]
    }
  ]
}